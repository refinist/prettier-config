  "description": "Out-of-the-box prettier configuration",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "homepage": "https://github.com/refinist/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/refinist/prettier-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/refinist/prettier-config.git"
  },
  "license": "MIT",
  "author": "Jeff Wang",
